                                           android:fontFamily="@font/product_sans_regular"
                                           android:text="Incects"
                                           android:textColor="@color/colorPrimary"
                                           android:textStyle="bold"
                                           app:chipBackgroundColor="#EEF1FB"
                                           app:chipIcon="@drawable/ic_baseline_arrow_forward_primary_24"
                                           app:chipIconTint="@color/colorPrimary"
                                           app:chipStrokeColor="#EEF1FB"
                                           app:chipStrokeWidth="1dp"
                                           app:rippleColor="@color/chip_ripple">

                                       </com.google.android.material.chip.Chip>
                                   </com.google.android.material.chip.ChipGroup>
                               </LinearLayout>
                            </LinearLayout>
                        </androidx.cardview.widget.CardView>
                    </LinearLayout>
                </LinearLayout>

            </RelativeLayout>
        </LinearLayout>
