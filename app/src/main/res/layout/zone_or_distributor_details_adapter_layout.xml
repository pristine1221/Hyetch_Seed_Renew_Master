<?xml version="1.0" encoding="UTF-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="3dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <FrameLayout
            android:id="@+id/circuler_text_section"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:layout_gravity="center_vertical"
            android:layout_margin="2dp"
            android:background="@drawable/circle_humidity_blue_icon"
            android:elevation="2dp"
            android:padding="5dp">

            <TextView
                android:id="@+id/tv_character_ofImageView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textColor="@color/my_app_background_color"
                android:textSize="25dp" />

        </FrameLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:orientation="vertical"
            android:paddingLeft="16dp"
            android:paddingTop="7dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/season_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Season : "
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_season"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular" />

                <TextView
                    android:id="@+id/booking_qty_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:layout_marginLeft="5dp"
                    android:text=" , Booking Qty : "
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_booking_qty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular" />


                <TextView
                    android:id="@+id/order_description_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Description : "
                    android:visibility="gone"
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_order_description"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:fontFamily="@font/product_sans_regular" />
            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Distributor name:"
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_distributor_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/zone_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Zone : "
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_zone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/bookind_ind_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Booking Ind No : "
                    android:textColor="@color/dark_gray"
                    android:visibility="visible"/>

                <TextView
                    android:id="@+id/tv_booking_indent_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:visibility="visible"/>

                <TextView
                    android:id="@+id/supply_allotted_qty_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Allotted Qty: "
                    android:textColor="@color/dark_gray"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/tv_supply_allotted_qty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:fontFamily="@font/product_sans_regular" />

                <TextView
                    android:id="@+id/order_booking_no_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Booking No : "
                    android:textColor="@color/dark_gray"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/tv_order_booking_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:visibility="gone"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/mkt_ind_nd_crop_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/marketing_ind_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Marketing Ind No. : "
                    android:textColor="@color/dark_gray"
                    android:visibility="visible"/>

                <TextView
                    android:id="@+id/tv_marketing_indent_no"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="visible"
                    android:fontFamily="@font/product_sans_regular" />

                <TextView
                    android:id="@+id/supply_supplies_qty_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Supplies Qty: "
                    android:textColor="@color/dark_gray"
                    android:visibility="gone"/>

                <TextView
                    android:id="@+id/tv_supply_supplies_qty"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:fontFamily="@font/product_sans_regular" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text=" , Crop : "
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_crop_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/order_field_created_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/order_created_on_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Created On: "
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_order_created_on"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/status_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text="Status : "
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular" />

                <TextView
                    android:id="@+id/order_crop_name_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/product_sans_regular"
                    android:text=" , Crop : "
                    android:visibility="gone"
                    android:textColor="@color/dark_gray" />

                <TextView
                    android:id="@+id/tv_order_crop_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:fontFamily="@font/product_sans_regular" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

</LinearLayout>